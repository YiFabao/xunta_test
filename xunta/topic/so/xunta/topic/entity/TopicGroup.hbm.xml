<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="topicgroup" name="so.xunta.topic.entity.TopicGroup">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="topicId" not-null="false"/>
    <property name="topicMemberId" not-null="false" />
    <property name="topicMemberName" not-null="false" />
    <property name="joinDatetime" not-null="false"/>
  </class>
</hibernate-mapping>

