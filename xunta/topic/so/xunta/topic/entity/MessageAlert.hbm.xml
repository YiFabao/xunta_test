<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="messageAlert" name="so.xunta.topic.entity.MessageAlert">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="authorId" not-null="false"/>
    <property name="topicContent" not-null="false" />
    <property name="_fromUserId" not-null="false"/>
    <property name="_fromUsername" not-null="false" />
    <property name="topicId" not-null="false" />
    <property name="datetime" not-null="false" />
    <property name="isHandle" not-null="false" />
    <property name="isRead" not-null="false" />
  </class>
</hibernate-mapping>

