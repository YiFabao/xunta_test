<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="topic" name="so.xunta.topic.entity.Topic">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="topicId" not-null="false"/>
    <property name="userId" not-null="false" />
    <property name="userName" not-null="false"/>
    <property name="topicName" not-null="false" />
    <property name="topicContent" not-null="false" />
    <property name="join_people_num" not-null="false" />
    <property name="logo_url" not-null="false" />
    <property name="createTime" not-null="false" />
    <property name="lastUpdateTime" not-null="false" />
  </class>
</hibernate-mapping>

