<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="topic_request_msg" name="so.xunta.topic.entity.TopicRequestMsg">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="fromUserId" not-null="false"/>
    <property name="toUserId" not-null="false" />
    <property name="fromUserName" not-null="false"/>
    <property name="toUserName" not-null="false" />
    <property name="topicId" not-null="false" />
    <property name="dateTime" not-null="false" />
    <property name="isAgree" not-null="false" />
    <property name="isHandle" not-null="false" />
  </class>
</hibernate-mapping>

