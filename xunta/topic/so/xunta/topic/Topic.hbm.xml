<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="topic" name="so.xunta.topic.Topic">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="topicId" not-null="false"/>
    <property name="authorId" not-null="false" />
    <property name="authorName" not-null="false"/>
    <property name="topicContent" not-null="false" />
    <property name="topicCreatetime" not-null="false" />
  </class>
</hibernate-mapping>

