<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="user" name="so.xunta.entity.User">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="xunta_username" not-null="false" length="15"/>
    <property name="password" not-null="false" length="18"/>
    <property name="email" not-null="false" length="18"/>
    <property name="qq_openId" not-null="false" />
    <property name="qq_accessToken" not-null="false" />
    <property name="weibo_uid" not-null="false" />
    <property name="weibo_accessToken" not-null="false"/>
    <property name="createTime" not-null="false" length="20"/>
    <property name="latestLoginTime" not-null="false" length="20"/>
  
  </class>
</hibernate-mapping>
