<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="topicmember" name="so.xunta.topic.TopicMember">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="topic_id" not-null="false"/>
    <property name="topic_member_id" not-null="false" />
    <property name="topic_member_name" not-null="false" />
    <property name="join_datetime" not-null="false"/>
    <property name="_exit" not-null="false" />
    <property name="exit_datetime" not-null="false" />
  </class>
</hibernate-mapping>

