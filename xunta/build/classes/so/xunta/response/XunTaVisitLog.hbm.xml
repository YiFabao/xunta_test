<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="xunta_visitlog" name="so.xunta.response.XunTaVisitLog">
    <id name="id">
      <generator class="native" />
    </id>
    <property name="ip" not-null="true" length="20"/>
    <property name="country" length="50"/>
    <property name="region" length="50"/>
    <property name="keywords" length="100"/>
    <property name="searchmode" length="20"/>
    <property name="date" type="int" length="8"/>
    <property name="time" type="int" length="6"/>
    <property name="code" length="30"/>
    <property name="usedtime" type="int" length="8"/>
  </class>
</hibernate-mapping>
